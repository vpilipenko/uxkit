{
  "name": "uxkit",
  "version": "1.0.0",
  "description": "some UX components",
  "main": "index.js",
  "scripts": {
    "install:components": "lerna exec npm install --concurrency 1",
    "build:components": "lerna exec npm run \"build\" --concurrency 1",
    "clean:components": "lerna exec npm run \"clean\"",
    "publish:components": "lerna publish",
    "analyze:components": "lerna bootstrap && lerna exec npm run \"analyze\"",
    "dev": "start-storybook -p 9001 -c ./.storybook",
    "clean": "rm -rf dist && mkdir dist && rm -rf .cache && ls | grep *.html | xargs rm"
  },
